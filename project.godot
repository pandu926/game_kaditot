; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Kadita Queen Of The Sea"
run/main_scene="res://scene/splash.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/logo/log-Photoroom.png"
config/icon="res://icon.svg"
boot_splash/minimum_display_time=1
run/use_filter=true
run/fullsize=true
run/image=""
run/show_image=true
run/minimum_display_time=0
run/bg_color=Color(0.14, 0.14, 0.14, 1)

[audio]

driver/driver="dsound"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
Check="*res://script/Check.gd"
Savemanager="*res://script/savemanager.gd"

[display]

window/size/viewport_width=2400
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/per_pixel_transparency/allowed=true
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg")

[global_group]

player=""

[input]

jump={
"deadzone": 0.5,
"events": []
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[internationalization]

locale/translations_pot_files=PackedStringArray("res://dialogue.dialogue", "res://dialog/manbuk.dialogue", "res://dialog/tutorLompat.dialogue", "res://dialog/masukPunden.dialogue", "res://dialog/dialogueRaja.dialogue", "res://dialog/dialogueStage2.dialogue")

[rendering]

textures/vram_compression/import_etc2_astc=true
